{
    "pageTitle": {
        "class": "pageTitle cf",
        "image": {
            "src": "image/icon(waterdrop100).png"
        },
        "title": {
            "text": "個人設定"
        },
        "summary": {
            "text": ""
        }
    },
    "optionForm":{
    	"class":"myOption optionForm"
    },
    "myOptionButtons": {
        "myOptionBackButton": {
        	"class":"myOptionBackButton back",
            "text": "戻る",
            "type":"button"
        },
        "myOptionConfirmChangeButton": {
        	"class":"myOptionConfirmChangeButton optionSave",
            "text": "更新",
            "type":"button"
        }
    },
    "myOptionNickname": {
        "class": "myOptionText",
        "myOptionLabel": {
            "text": "あなたのニックネーム"
        },
        "myOptionTextbox": {
            "type": "text",
            "name": "nickname",
            "value":"user"
        }
    },
    "myOptionTel": {
        "class": "myOptionText",
        "myOptionLabel": {
            "text": "あなたの電話番号"
        },
        "myOptionTextbox": {
            "type": "number",
            "name": "tel",
                    "style": "ime-mode:disabled",
                    "type": "tel",
                    "onkeyDown": "return controllInputChar(event);",
                    "value":"090-0000-0000"
        }
    },
    "myOptionAddress": {
        "class": "myOptionText myOptionAddress",
        "myOptionLabel": {
            "text": "あなたの住所"
        },
        "myOptionTextbox": {
            "type": "text",
            "name": "address",
            "value":"東京都新宿区新宿7丁目1-4 猫の目ハイツ505号室"
        }
    },
    "myOptionMail": {
        "class": "myOptionText myOptionTextConfirm myOptionMail",
        "myOptionLabel": {
            "text": "メールアドレス"
        },
        "myOptionTextbox":{
            "type": "email",
            "name":"email",
            "style": "ime-mode:disabled",
            "onkeyDown": "return controllInputChar(event);",
            "aaa@bbb.ccc"
        }
    },
    "myOptionPassword": {
        "class": "myOptionText myOptionTextConfirm myOptionPassword",
        "myOptionLabel": {
            "text": "パスワード"
        },
        "myOptionTextbox": {
            "type": "password",
            "name": "password",
            "style": "ime-mode:disabled"
        }
    },
    "myOptionBlogTitle": {
        "class": "myOptionText",
        "myOptionLabel": {
            "text": "ブログタイトル"
        },
        "myOptionTextbox": {
            "type": "text",
            "name": "blogTitle"
        }
    },
    "myOptionBlogPublication": {
        "class": "myOptionRadio",
        "myOptionLabel": {
            "text": "ブログ公開範囲"
        },
        "myOptionRadio1": {
            "class": "myOptionRadio",
            "type": "radio",
            "name": "blogPublication",
            "value": "0",
            "checked":"false"
        },
        "myOptionLabelInline1": {
            "class": "myOptionLabelInline",
            "text": "全体"
        },
        "myOptionRadio2": {
            "class": "myOptionRadio",
            "type": "radio",
            "name": "blogPublication",
            "value": "1",
            "checked":"false"
        },
        "myOptionLabelInline2": {
            "class": "myOptionLabelInline",
            "text": "友達"
        },
        "myOptionRadio3": {
            "class": "myOptionRadio",
            "type": "radio",
            "name": "blogPublication",
            "value": "2",
            "checked":"false"
        },
        "myOptionLabelInline3": {
            "class": "myOptionLabelInline",
            "text": "非公開"
        }
    }
}