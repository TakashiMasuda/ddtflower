    },
    "myBlogTitle":{
        "class":"myBlogTitle myPageTitle",
        "text":"ブログ"
    },
    "myBlogButtonArea":{
        "createArticleButton":{
            "type":"button",
            "value":"記事作成"
        },
        "editArticleButton":{
            "type":"button",
            "value":"記事編集"
