            "value":""
        },
        "config": {
            "columns": {
                "id":{
                    "className":"replaceCheckbox"
                },
                "date": {
                    "columnName": "日付"
                },
                "title": {
                    "columnName": "タイトル"
                }
            }
        }
    },
    "deleteMyBlog":{
        "db_setQuery":"DELETE FROM user_blog WHERE id = 'id';",
        "id":{
            "value":""
        }
    }
