    "lecturePermitListInfoTable":{
        "db_getQuery": "(SELECT user_inf.id AS id ,user_name ,lesson_name ,user_classwork.user_classwork_cost AS cost ,user_classwork.use_point AS use_point ,user_classwork.level_no AS level_no ,user_classwork.stage_no AS stage_no ,1 AS sell_number ,'' AS content FROM user_classwork INNER JOIN classwork ON classwork.id = user_classwork.classwork_key INNER JOIN time_table_day ON time_table_day.id = classwork.time_table_day_key AND time_table_day.lesson_date <= 'toDate' AND time_table_day.lesson_date >= 'FromDate' INNER JOIN user_inf ON user_inf.id = user_classwork.user_key INNER JOIN  lesson_inf ON lesson_inf.id = classwork.lesson_key ORDER BY time_table_day.lesson_date DESC) UNION ALL (SELECT user_inf.id AS id ,user_name ,'' AS lesson_name ,pay_cash AS cost ,commodity_sell.use_point AS use_point ,1 AS level_no ,1 AS stage_no ,sell_number ,content FROM commodity_sell INNER JOIN user_inf ON user_inf.id = commodity_sell.user_key AND sell_datetime <= 'toDate' AND sell_datetime >= 'FromDate' ORDER BY sell_datetime DESC);",
        "FromDate":{
            "value":"2015-03-01"
        },
        "toDate":{
            "value":"2015-03-31"
        },
        "config": {
            "columns": {
                "No": {
                    "columnName": "No."
                },
                "user_name": {
                    "columnName": "名前"
                },
                "lesson_name": {
                    "columnName": "内容"
                },
                "cost": {
                    "columnName": "入金(円)",
                    "className":"replaceTextbox"
                },
                "use_point": {
                    "columnName": "使用pt(pt)",
                    "className":"replaceTextbox"
                },
                "stage_no":{
                    "style":"display:none;"
                },
                "level_no":{
                    "style":"display:none;"
                },
                "sell_number":{
                    "style":"display:none;"
                },
                "content":{
                    "style":"display:none;"
                }
            }
        }
    },
