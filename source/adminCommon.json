    "userInfoTable":{
        "db_getQuery": "SELECT time_table_day.id AS time_table_key ,time_table_day.lesson_date AS lesson_date ,start_time ,end_time ,lesson_name ,user_name ,user_classwork.stage_no ,user_classwork.level_no FROM time_table_day INNER JOIN classwork ON time_table_day.id = classwork.time_table_day_key AND time_table_day.lesson_date = 'date' INNER JOIN user_classwork ON classwork.id = user_classwork.classwork_key INNER JOIN user_inf ON user_inf.id = user_classwork.user_key INNER JOIN  lesson_inf ON lesson_inf.id = classwork.lesson_key INNER JOIN timetable_inf ON timetable_inf.id = time_table_day.timetable_key ORDER BY start_time ASC;",
        "date": {
            "value": "2015-04-10"
        },
        "config": {
            "columns": {
                "time_table_key": {
                    "columnName": "No."
                },
                "lesson_date":{
                    "style":"display:none;"
                },
                "start_time": {
                    "columnName": "開始"
                },
                "end_time": {
                    "columnName": "終了"
                },
                "lesson_name": {
                    "columnName": "テーマ"
                },
                "user_name": {
                    "columnName": "名前"
                },
                "stage_no": {
                    "columnName": "Stg-Lv",
                    "style":"display:none;"
                },
                "level_no": {
                    "columnName": "レベル",
                    "style":"display:none;"
                }
            }
        }
    },
