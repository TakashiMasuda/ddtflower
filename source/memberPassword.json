{
    "passArea":{
        "class":"passArea",
        "pageHeader":{
            "text":"パスワード変更"
        },
        "postPass":{
            "newPassArea":{
                "passLabel":{
                    "class":"passLabel inlineBlock",
                    "text":"新しいパスワード"
                },
                "passTextbox":{
                    "class":"passTextbox inlineBlock newPassword",
                    "type":"password",
                    "name":"newPass"
                }
            },
            "confirmArea":{
                "passLabel":{
                    "class":"passLabel inlineBlock",
                    "text":"確認(再入力)"
                },
                "passTextbox":{
                    "class":"passTextbox inlineBlock confirmPassword",
                    "type":"password",
                    "name":"password"
                }
            },
            "cancelButton":{
                "class":"cancelButton fRight",
                "cancelButtonLink":{
                    "href":"#memberProfile.html",
                    "text":"キャンセル"
                }
            },
            "updateButton":{
                "class":"passwordUpdate fRight",
                "type":"button",
                "value":"更新"
            }
        }
    },
    "updatePassword":{
        "db_setQuery":"UPDATE user_inf SET password = 'password',update_datetime = NOW() WHERE id = 'userId';"
    }
}